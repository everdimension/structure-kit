  "license": "MIT",
  "bugs": {
    "url": "https://github.com/everdimension/structure-kit/issues"
  },
  "homepage": "https://github.com/everdimension/structure-kit#readme",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
  }
